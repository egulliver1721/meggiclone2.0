{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "dev": {
        "cache": false
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "test": {
        "dependsOn": ["^build"]
      },
      "deploy": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      }
    }
  }